<!DOCTYPE html>
<head>
    
</head>
<body class="contact-body">

    <div class="row">
      <br><br><br><br><br><br><br><div class="col-md-4 col-md-offset-7">
        
          <%= form_for @contact do |f| %>
            <div class="form-group">
              <%= f.label :name, class: 'quicksand' %>
              <%= f.text_field :name, class: 'form-control' %>
            </div>
            <br>
            <div class="form-group">
              <%= f.label :email, class: 'quicksand' %>
              <%= f.email_field :email, class: 'form-control' %>
            </div>
            <br>
            <div class="form-group">
              <%= f.label :comments, class: 'quicksand' %>
         
              <%= f.text_area :comments, class: 'form-control comments-area' %>
             
            </div>
            <%= f.submit 'Send this to Josh', class: 'btn btn-primary submit-btn quicksand' %>
          <% end %>
        
      </div>
    </div>

</body>
</html>