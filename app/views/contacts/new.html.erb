<!DOCTYPE html>
<head>
    
</head>
<body>
<div class="row">
  <br><br><br><br><br><br><br><div class="col-md-4 col-md-offset-7">
    
      <%= form_for @contact do |f| %>
        <div class="form-group">
          <%= f.label :name %>
          <%= f.text_field :name, class: 'form-control' %>
        </div>
        <br>
        <div class="form-group">
          <%= f.label :email %>
          <%= f.email_field :email, class: 'form-control' %>
        </div>
        <br>
        <div class="form-group">
          <%= f.label :comments %>
          <%= f.text_area :comments, class: 'form-control' %>
        </div>
        <%= f.submit 'Send this to Josh', class: 'btn btn-primary submit-btn' %>
      <% end %>
    
  </div>
</div>
</body>
</html>